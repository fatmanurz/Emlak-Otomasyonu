@model  List<double>
<table>
    <thead>
        <tr>
            <th>Kira Fiyatı</th>
            <th>Tarih</th>
        </tr>
    </thead>
    <tbody>
      @{ 
          var gun = 0;
          foreach (double kur in Model)
          {
              gun--;
            <tr>
                <td>@kur.ToString("#.##")</td>
                <td>@DateTime.Today.AddDays(gun).ToShortDateString()</td>
            </tr>
                }
        }
    </tbody>
</table>
